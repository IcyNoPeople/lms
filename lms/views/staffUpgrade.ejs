    <!-- end left sidebar -->
    <!-- ============================================================== -->
    <!-- ============================================================== -->
    <!-- wrapper  -->
    <!-- ============================================================== -->
    <div class="dashboard-wrapper">
        <div class="influence-profile">
            <div class="container-fluid dashboard-content ">
                <!-- ============================================================== -->
                <!-- pageheader -->
                <!-- ============================================================== -->
                <div class="row">
                    <div class="col-xl-12 col-lg-12 col-md-12 col-sm-12 col-12">
                        <div class="page-header">
                            <h3 class="mb-2">Your Profile </h3>
                            <p class="pageheader-text"></p>
                            <div class="page-breadcrumb">
                                <nav aria-label="breadcrumb">
                                    <ol class="breadcrumb">
                                        <li class="breadcrumb-item"><a href="#" class="breadcrumb-link">Dashboard</a>
                                        </li>
                                        <li class="breadcrumb-item active" aria-current="page">Your Profile (User)
                                        </li>
                                    </ol>
                                </nav>
                            </div>
                        </div>
                    </div>
                </div>
                <!-- ============================================================== -->
                <!-- end pageheader -->
                <!-- ============================================================== -->
                <!-- ============================================================== -->
                <!-- content -->
                <!-- ============================================================== -->
                <div class="row">
                    <!-- ============================================================== -->
                    <!-- profile -->
                    <!-- ============================================================== -->
                    <div class="col-xl-12 col-lg-12 col-md-12 col-sm-12 col-12">
                        <div class="card">
                            <div class="card-header">
                                <%= staffRes.first_name %> <%= staffRes.last_name %>
                            </div>
                            <div class="card-body">
                                <blockquote class="blockquote mb-0">
                                    <p>Staff ID: <%= staffID %>
                                    Department: <%= staffRes.name %> <br/>
                                    Position: <%= staffRes.position %>
                                    Access: <br/>
                                        <ol start="1">
                                            <% accessRes.forEach((accessRess) => { %>
                                            <li> <%= accessRess.name %> </li>            
                                            <% }) %>
                                        </ol>
                                    </p>
                                </blockquote>
                            </div>
                        </div>
                    </div>
                    <!-- ============================================================== -->
                    <!-- end profile -->
                    <!-- ============================================================== -->
                    <!-- ============================================================== -->
                    <!-- campaign data -->
                    <!-- ============================================================== -->
                    <div class="col-xl-12 col-lg-12 col-md-7 col-sm-12 col-12">
                        <!-- ============================================================== -->
                        <!-- campaign tab one -->
                        <!-- ============================================================== -->
                        <div class="influence-profile-content pills-regular">
                            <div class="col-xl-12 col-lg-12 col-md-12 col-sm-12 col-12">
                                <div class="section-block">
                                    <h3 class="section-title"><%= staffRes.name %></h3>
                                </div>
                            </div>
                            <div class="col-xl-3 col-lg-3 col-md-6 col-sm-12 col-12">
                                <div class="card">
                                    <div class="card-body">
                                        <h1 class="mb-1">9</h1>
                                        <p>On going Loans</p>
                                    </div>
                                </div>
                            </div>
                            
                            <div class="col-xl-3 col-lg-3 col-md-6 col-sm-12 col-12">
                                <div class="card">
                                    <div class="card-body">
                                        <h1 class="mb-1"><%= ></h1>
                                        <p>Pending Loans</p>
                                    </div>
                                </div>
                            </div>

                            <div class="tab-content" id="pills-tabContent">
                               
                                <div class="tab-pane fade" id="pills-packages" role="tabpanel"
                                    aria-labelledby="pills-packages-tab">
                                    <div class="col-xl-12 col-lg-12 col-md-12 col-sm-12 col-12">
                                        <div class="card">
                                            <div class="card-header">
                                                <h5 class="mb-0">Loan Overview</h5>
                                                <p>Here is an overview of your Loans</p>
                                            </div>
                                            <div class="card-body">
                                                <div class="table-responsive">
                                                    <table id="example"
                                                        class="table table-striped table-bordered second"
                                                        style="width:100%">
                                                        <thead>
                                                            <tr>
                                                                <th>Loan ID</th>
                                                                <th>Loan Type</th>
                                                                <th>Loan Tenure (Years)</th>
                                                                <th>Interest Rate (Yr)</th>
                                                                <th>Principal Amount</th>
                                                                <th>Total Paid</th>
                                                                <th>Loan Status</th>

                                                            </tr>
                                                        </thead>
                                                        <tbody>

                                                           
                                                            <tr>
                                                                <td align="center">l</td>
                                                                <td align="center">o</td>
                                                                <td align="center">l</td>
                                                                <td align="center">h</td>
                                                                <td align="center">e</td>
                                                                <td align="center">h</td>
                                                                <td align="center">e</td>
                                                            </tr>
                                                            

                                                            

                                                        </tbody>
                                                        <tfoot>
                                                            <tr>
                                                                <th>Loan ID</th>
                                                                <th>Loan Type</th>
                                                                <th>Loan Tenure (Years) </th>
                                                                <th>Interest Rate (Yr)</th>
                                                                <th>Principal Amount</th>
                                                                <th>Total Paid</th>
                                                                <th>Loan Status</th>
                                                            </tr>
                                                        </tfoot>
                                                    </table>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>

                            </div>
                        </div>
                        <!-- ============================================================== -->
                        <!-- end campaign tab one -->
                        <!-- ============================================================== -->
                    </div>
                    <!-- ============================================================== -->
                    <!-- end campaign data -->
                    <!-- ============================================================== -->
                </div>
            </div>
        </div>
        <!-- ============================================================== -->
        <!-- end content -->
        <!-- ============================================================== -->
    </div>
